{
    "name": "Archive Page",
    "author": "John Navas",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
      ],
    "optional_permissions": [ "notifications" ],
    "version": "0.9.0",
    "description": "Archive webpage with archive.today",
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "browser_action": {
        "default_icon": {
            "16": "images/Share2Archive-16.png",
            "24": "images/Share2Archive-24.png",
            "32": "images/Share2Archive-32.png"
        },
        "default_title": "Archive Page"
    },
    "icons": {
        "16": "images/ArchivePage-16.png",
        "24": "images/ArchivePage-24.png",
        "32": "images/ArchivePage-32.png",
        "48": "images/ArchivePage-48.png",
        "128": "images/ArchivePage-128.png"
    },
    "manifest_version": 2
}
